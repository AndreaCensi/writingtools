@Article{rogister12asynchronous,
  Title                    = {Asynchronous Event-Based Binocular Stereo Matching},
  Author                   = {Rogister, P. and Benosman, R. and Sio-Hoi Ieng and Lichtsteiner, P. and Delbruck, T.},
  Year                     = {2012},
