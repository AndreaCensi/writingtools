@inproceedings{censi09hsm3d,
  author = {Andrea Censi and Stefano Carpin},
  title = {{HSM3D}: Feature-Less Global {6DOF} Scan-Matching in the {H}ough/{R}adon
	Domain },
  booktitle = icra,
  year = {2009},
  address = {Kobe, Japan},
  abstract = { This paper presents HSM3D, an algorithm for global rigid6DOF alignment
	of 3D point clouds. The algorithm works byprojecting the two input
	sets into the Radon/Hough domain,whose properties allow to decompose
	the 6DOF search into aseries of fast one-dimensional cross-correlations.
	Noplanes or other particular features must be present in theinput
	data, and the algorithm is provably complete in thecase of noise-free
	input. The algorithm has beenexperimentally validated on publicly
	available data sets. },
  eprint = {http://purl.org/censi/research/2009-icra-hsm3d-slides.pdf},
